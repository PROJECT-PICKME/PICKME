<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="project.pickme.unit.repository.UnitMapper">
    <select id = "findAll" resultType="project.pickme.unit.domain.Unit">
        select
        u.start_scope as startScope,
        u.end_scope as endScope,
        u.price_unit as priceUnit

        from Unit u
    </select>
</mapper>
